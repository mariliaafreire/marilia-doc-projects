Experimental Plan 
Goals {
	G1 "Analyze the test execution process, for the purpose of evaluating two different SPL test case design techniques (GT vs. ST), with respect to their efficiency regarding
          time to execute the test suites as well as the number of terminated CRs reported during the test execution process. Using the point of view of test engineers and 
          software engineering researchers in the context of controlled experiments done with graduate and undergraduate students."
}
Hypotheses {
	H10 "The test execution effort using ST is the same compared with the GT " null from G1 {
		H101 TestExecutionTime( TestTechnique.Specific) = TestExecutionTime ( TestTechnique.Generic)
	}
	H11 "The test execution effort using ST is better than using the GT " alternative from G1 {
	H111 TestExecutionTime( TestTechnique.Specific) <= TestExecutionTime ( TestTechnique.Generic)
	}
	H20 "The number of terminated CRs using ST is the same compared with the GT" null from G1{
		H201 NumberofCR (TestTechnique.Specific) = NumberofCR ( TestTechnique.Generic)
	}
	H21 "The number of terminated CRs using ST is the same compared with the GT" alternative from G1 {
		H211 NumberofCR ( TestTechnique.Specific) >= NumberofCR ( TestTechnique.Generic)
	}
}

DesignOfExperiment = LS - Latin Square {
	
	
	Parameter SPLComplexity = "low"
    Parameter ParticipantLevel = "Undergraduating Students"
    
    Dependent Variable TestExecutionTime "Test execution time for each test in the Test Suite" TimeExecution1 TimeExecution2 TimeExecution3 TimeExecution4 
    Dependent Variable NumberofCR "Number of terminated CR" ReportedCR1 ReportedCR2 ReportedCR3 ReportedCR4
    
    Factor TestTechnique isDesiredVariation True {
                Generic
                Specific
        }
    Factor Feature isDesiredVariation False { 
                F1
                F2
         }
    Factor Participants isDesiredVariation False {
         		Participant1
         		Participant2
         }
   
   Statistical Analysis Technique H10.H101 H11.H111 : ANOVA
   								 H20.H201 H21.H211 : Others
   
   Internal Replication  4
   
}

