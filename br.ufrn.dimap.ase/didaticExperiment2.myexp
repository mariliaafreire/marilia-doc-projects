Experiment "2 ยบ Comparison of Java and C++" 

    Process "RCBD_ComparisonProcessJava-PhoneBook"{
   	
    Activity "PhoneBook_UseCaseProject" "Create the project class diagram based on the use case specification and in the project architecture" 
    		"PhoneBook_ImplementUseCase" {
    	Task "DesignClassDiagram" description "Design Class Diagram" 
    	 	artefacts artefact "PhoneBook_UseCaseSpecfication" description "Description of Use Case" type input;
					  artefact "PhoneBook_ClassDiagram" description "Class Diagram" type output; Role "Subject";;
		Task "DesignScreens" description "Design of Screens" 
			artefacts  artefact "PhoneBook_UseCaseSpecfication" description "Description of Use Case" type input;
					   artefact "PhoneBook_PrototypeInterface" description "Prototype of interface" type output; Role "Subject";;
	}
	Activity "PhoneBook_ImplementUseCase" "Implementation of Use Case" "UseCaseTest"{
		 Task "CodifyJavaPhoneBook_UseCase"description "Create the PhoneBook UseCase Java code"
		 artefacts artefact "ClassDiagram" description "Class Diagram" type input;
    	      		artefact "PhoneBook_PrototypeInterface" description "Prototype of interface" type input; 
    	      		artefact "PhoneBook_JavaUseCaseCode" description "Implementation of Java Use Case" type output; Role "Subject";;
	}
	Activity "UseCaseTest" "Perform Test of Use Case"	{
		Task "CreateJavaPhoneBookTestCases" description "Develop the test cases and test data for the requirements to be tested" 
				artefacts  artefact "PhoneBook_JavaUseCaseSpecfication" description "Description of Use Case" type input;
						   artefact "PhoneBook_JavaTestCase" description "Specification of a set of test"  type output; Role "Subject";;
		Task "PhoneBook_RunTests" description "Run the appropriate test cases, analyze results" 
				artefacts artefact "JavaTestCase" description "Specification of a set of test"  type input; 
						artefact "PhoneBook_JavaUseCaseCode" description "Implementation of Java Use Case" type input;
					   artefact "PhoneBook_TestLog" description "Report that is captured during a run of tests" type output; Role "Subject";;
	}
	
	}

