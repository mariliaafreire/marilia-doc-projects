<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="iceis">
<swimlane name="manager">
	<assignment actor-id="manager"/>
</swimlane>
	<start-state name="Inicio">
		<task name="Inicio" swimlane="manager">
			<controller>			
				<variable  name="comentario" access="read,write,required"/>
			</controller>
		</task>
		<transition name="starttofork" to="fork1"></transition>
		<event type="node-leave">
			<action class="com.sample.action.InicioVariaveisActionHandler">
				<log></log>
			</action>
		</event>
	</start-state>
	
	<task-node name="test_solution" >
		 
			<task name="test_solution" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write,required"/>
				<variable  name="developTime" access="read,write"/>
			  </controller>
				
			</task>
			<transition name="testTransition" to="joinEnd"/>
		
			<event type="task-end">
					<action name="Duration of all cdu devlopment tasks" class="com.sample.action.DevelopActionHandler">
					</action>
			</event>
	</task-node>
	<task-node name="plan_manage_iteration" >
		 
			<task name="plan_manage_iteration" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write"/>
			  </controller>
				
			</task>
			<transition name="planTransition" to="joinEnd"/>
		
	</task-node>
	<task-node name="develop_architecture" >
		<task name="develop_architecture" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write,required"/>
			  </controller>
				
			</task>
			<transition name="developTransition" to="joinEnd"/>
		
	</task-node>
	<task-node name="ongoing_tasks" >
		 
			<task name="ongoing_tasks" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write,required"/>
			  </controller>
				
			</task>
			<transition name="ongoingTransition" to="joinEnd"/>
		
	</task-node>
		<task-node name="identify_and_refine_requirements" >
			<task name="identify_and_refine_requirements" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write,required"/>
				<variable  name="requirementTime" access="read,write"/>
			  </controller>
			</task>
			<event type="task-end">
					<action class="com.sample.action.RequirementsActionHandler">
					</action>
			</event>
			<transition name="identify_and_refine_requirementstransition" to="joinEnd"/>
	</task-node>
	<task-node name="develop_solution" >
			<task name="develop_solution" swimlane="manager">
			  <controller>			
				<variable  name="comentario" access="read,write,required"/>
				<variable  name="developTime" access="read,write"/>
			  </controller>
			</task>
			<transition name="developSTransition" to="joinEnd"/>
			<event type="task-end">
					<action name="Duration of all cdu devlopment tasks" class="com.sample.action.DevelopActionHandler">
					</action>
			</event>
	</task-node>

	<join name="joinEnd">
			<transition name="to_EndTransition" to="End"/>
	</join>

	<fork name="fork1">
		<transition to="identify_and_refine_requirements" name="startidentify_and_refine_requirementstransition"></transition>
		<transition to="develop_architecture" name="startdevelop_architecturetransition"></transition>
		<transition to="develop_solution" name="startdevelop_solutiontransition"></transition>
		<transition to="test_solution" name="starttest_solutiontransition"></transition>
		<transition to="plan_manage_iteration" name="startplan_manage_iterationtransition"></transition>
		<transition to="ongoing_tasks" name="startongoing_taskstransition"></transition>
	</fork>


	<end-state name="End"/>
</process-definition>
